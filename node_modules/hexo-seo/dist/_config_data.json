{
  "cache": true,
  "js": {
    "enable": true,
    "concat": false,
    "exclude": [
      "*.min.js",
      "**/*.min.js"
    ],
    "options": {
      "compress": {
        "dead_code": true
      },
      "mangle": {
        "toplevel": true,
        "safari10": true
      }
    }
  },
  "css": {
    "enable": true,
    "exclude": [
      "*.min.css",
      "**/*.min.css"
    ]
  },
  "html": {
    "enable": true,
    "fix": true,
    "exclude": [
      "*.min.{htm,html}"
    ],
    "collapseBooleanAttributes": true,
    "collapseWhitespace": true,
    "ignoreCustomComments": [
      {}
    ],
    "removeComments": true,
    "removeEmptyAttributes": true,
    "removeScriptTypeAttributes": true,
    "removeStyleLinkTypeAttributes": true,
    "minifyJS": true,
    "minifyCSS": true
  },
  "img": {
    "enable": true,
    "default": "https://upload.wikimedia.org/wikipedia/commons/6/65/No-Image-Placeholder.svg",
    "onerror": "serverside",
    "broken": false
  },
  "host": "localhost:4000",
  "links": {
    "blank": true,
    "enable": true,
    "allow": [
      "webmanajemen.com"
    ],
    "exclude": [
      "webmanajemen.com",
      "web-manajemen.blogspot.com"
    ]
  },
  "schema": {
    "sitelink": {
      "enable": true,
      "searchUrl": "https://www.webmanajemen.com/search?q={search_term_string}"
    },
    "article": {
      "enable": true
    },
    "breadcrumb": {
      "enable": true
    },
    "homepage": {
      "enable": true
    }
  },
  "sitemap": {
    "yoast": true,
    "gnews": true
  },
  "theme_dir": "d:/Repositories/hexo-seo/tmp/site/themes/landscape",
  "source_dir": "d:/Repositories/hexo-seo/tmp/site/source",
  "public_dir": "d:/Repositories/hexo-seo/tmp/site/public",
  "post_dir": "d:/Repositories/hexo-seo/tmp/site/source/_posts",
  "search": {
    "type": [
      "post",
      "page",
      "page",
      "post"
    ]
  },
  "feed": {
    "type": [
      "post",
      "page",
      "page",
      "post"
    ],
    "icon": "https://w7.pngwing.com/pngs/745/306/png-transparent-gallery-image-images-photo-picture-pictures-set-app-incredibles-icon-thumbnail.png"
  }
}